<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jude.demo" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.yjs.demo.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/MainTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yjs.demo.ListActivity" android:label="ScrollActivity"/>
        <activity android:name="com.yjs.demo.TextActivity" android:label="StaticActivity"/>
        <activity android:name="com.yjs.demo.ViewPagerActivity" android:label="ViewPagerActivity"/>
    </application>

</manifest>
